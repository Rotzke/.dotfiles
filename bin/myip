#!/bin/bash

if [ `gsettings get org.gnome.system.proxy mode` == "'manual'" ]; then echo `curl -H 'Cache-Control: no-cache' -s -x socks://127.0.0.1:9050 http://freegeoip.net/json/ | cut -d'"' -f 4,12 | tr '"' '/'`; else echo `curl -H 'Cache-Control: no-cache' -s http://freegeoip.net/json/ | cut -d'"' -f 4,12 | tr '"' '/'`; fi
